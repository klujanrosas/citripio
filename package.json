{
  "name": "citripio",
  "description": "A barebones file system logger with crypto sprinkled on top",
  "author": "Kenneth Lujan <kenneth.lujan@kw.com> (https://github.com/klujanrosas)",
  "scripts": {
    "start": "cross-env TIMESTAMP=true PORT=3000 node -r esm src/index.js",
    "test": "cross-env TIMESTAMP=false jest",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src"
  },
  "dependencies": {
    "crypto-js": "^4.0.0",
    "esm": "^3.2.25",
    "fastify": "^3.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@types/jest": "^26.0.23",
    "babel-jest": "^27.0.5",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "get-port": "^5.1.1",
    "jest": "^27.0.5",
    "node-fetch": "^2.6.1",
    "pkg-dir": "^5.0.0",
    "prettier": "^2.3.1",
    "tree-kill": "^1.2.2"
  }
}
